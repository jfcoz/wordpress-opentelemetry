{
    "name": "jfcoz/wordpress-opentelemetry",
    "description": "An autoinstrumentation of wordpress with the FrankenPHP wordpress docker image",
    "type": "project",
    "minimum-stability": "beta",
    "require": {
        "open-telemetry/opentelemetry-auto-wordpress": "^0.0.16",
        "open-telemetry/sdk": "^1.0@beta",
        "open-telemetry/exporter-otlp": "^1.0@beta",
        "php-http/guzzle7-adapter": "^1.0",
        "grpc/grpc": "~1.57.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
